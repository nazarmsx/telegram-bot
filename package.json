{
  "name": "test-bot",
  "version": "0.1.0",
  "description": "Users API",
  "scripts": {
    "start": "npm run serve",
    "build": "npm run build-ts",
    "serve": "node dist/server.js",
    "start:bot": "cross-env NODE_ENV=kyivbot node dist/bot.js",
    "start:kyivbot:prod": "cross-env NODE_ENV=kyivbotprod node dist/bot.js",
    "start:bot:odesa": "cross-env NODE_ENV=odesabot node dist/bot.js",
    "start:odesabot:prod": "cross-env NODE_ENV=odesabotprod node dist/bot.js",
    "start:kharkivbot:prod": "cross-env NODE_ENV=kharkivbotprod node dist/bot.js",
    "watch-node": "nodemon dist/server.js",
    "test": "jest --forceExit --coverage --verbose",
    "watch-test": "npm run test -- --watchAll",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "restart": "tsc && pm2 restart kyiv-bot && pm2 restart odesa-bot"
  },
  "dependencies": {
    "@types/telebot": "^1.2.2",
    "async": "^3.0.1",
    "await-to-js": "^2.1.1",
    "axios": "^0.21.1",
    "bcrypt": "^3.0.7",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "date-fns": "^2.8.1",
    "dotenv": "^8.0.0",
    "errorhandler": "^1.5.1",
    "express": "^4.17.1",
    "express-validator": "^6.0.1",
    "lusca": "^1.6.1",
    "moment": "^2.29.1",
    "mongoose": "^5.6.0",
    "morgan": "^1.9.1",
    "reflect-metadata": "^0.1.13",
    "telebot": "^1.4.1",
    "tsyringe": "^4.0.1",
    "viber-bot": "^1.0.16",
    "winston": "^2.4.2"
  },
  "devDependencies": {
    "@types/async": "^3.0.0",
    "@types/bluebird": "^3.5.27",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/compression": "^0.0.36",
    "@types/dotenv": "^6.1.1",
    "@types/errorhandler": "^0.0.32",
    "@types/express": "^4.17.0",
    "@types/jest": "^24.0.15",
    "@types/lodash": "^4.14.134",
    "@types/lusca": "^1.6.0",
    "@types/mongodb": "^3.1.28",
    "@types/mongoose": "^5.5.6",
    "@types/morgan": "^1.7.35",
    "@types/node": "^12.0.10",
    "@types/supertest": "^2.0.7",
    "@types/winston": "^2.3.9",
    "@types/bcrypt": "^3.0.0",
    "@typescript-eslint/eslint-plugin": "^1.12.0",
    "@typescript-eslint/parser": "^1.12.0",
    "chai": "^4.2.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.0.0",
    "jest": "^24.8.0",
    "nodemon": "^1.19.1",
    "shelljs": "^0.8.3",
    "supertest": "^4.0.2",
    "ts-jest": "^24.0.2",
    "ts-node": "^8.3.0",
    "typescript": "^3.5.2"
  }
